{
  "manifest_version": 3,
  "name": "URL Rearrange",
  "version": "1.3.1",
  "description": "Split and combine URLs as you like.",
  "action": {
    "default_popup": "settings.html",
    "default_icon": {
      "16": "images/URL_rearrange_16.png",
      "32": "images/URL_rearrange_32.png",
      "48": "images/URL_rearrange_64.png",
      "128": "images/URL_rearrange_128.png"
    }
  },
  "icons": {
    "16": "images/URL_rearrange_16.png",
    "32": "images/URL_rearrange_32.png",
    "48": "images/URL_rearrange_64.png",
    "128": "images/URL_rearrange_128.png"
  },
  "permissions": [
    "tabs"
  ]
}
